{
    "name": "muskie9/silverstripe-selectboxfield",
    "description": "Selectbox dropdown field for silverstripe",
    "authors": [
        {
            "name": "Nic Horstmeier",
            "email": "nhorstmeier@gmail.com",
            "homepage": "http://muskie9.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "silverstripe",
        "SelectboxField"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "installer-name": "selectboxfield",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "expose": [
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "Muskie9\\SelectboxField\\": "src/",
            "Muskie9\\SelectboxField\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
